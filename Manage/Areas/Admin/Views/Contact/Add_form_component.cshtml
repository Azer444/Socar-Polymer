@model ContactFormComponent
<!-- Page Wrapper -->
<div id="wrapper">
    @await Html.PartialAsync("_SidebarPartial")
    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">
        <!-- Main Content -->
        <div id="content">
            @await Html.PartialAsync("_TopbarPartial")
            <!-- Begin Page Content -->
            <div class="container-fluid">
                <h1 class="main-color">Add Contact Form Component</h1>
                <div class="row main-color">
                    <div class="col-12">
                        <form asp-action="add_form_component" asp-controller="contact" asp-area="admin" method="post" enctype="multipart/form-data">
                            <div class="container">
                                <div class="row">
                                    <div class="col-12 mt-3">
                                        <h3 class="main-color">Title</h3>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Title_AZ" class="control-label"></label>
                                            <input asp-for="Title_AZ" class="form-control" />
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Title_RU" class="control-label"></label>
                                            <input asp-for="Title_RU" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Title_EN" class="control-label"></label>
                                            <input asp-for="Title_EN" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 mt-3">
                                        <h3 class="main-color">Subtitle</h3>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_1_AZ" class="control-label"></label>
                                            <input asp-for="Subtitle_1_AZ" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_1_RU" class="control-label"></label>
                                            <input asp-for="Subtitle_1_RU" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_1_EN" class="control-label"></label>
                                            <input asp-for="Subtitle_1_EN" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_2_AZ" class="control-label"></label>
                                            <input asp-for="Subtitle_2_AZ" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_2_RU" class="control-label"></label>
                                            <input asp-for="Subtitle_2_RU" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_2_EN" class="control-label"></label>
                                            <input asp-for="Subtitle_2_EN" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_3_AZ" class="control-label"></label>
                                            <input asp-for="Subtitle_3_AZ" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_3_RU" class="control-label"></label>
                                            <input asp-for="Subtitle_3_RU" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_3_EN" class="control-label"></label>
                                            <input asp-for="Subtitle_3_EN" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_4_AZ" class="control-label"></label>
                                            <input asp-for="Subtitle_4_AZ" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_4_RU" class="control-label"></label>
                                            <input asp-for="Subtitle_4_RU" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Subtitle_4_EN" class="control-label"></label>
                                            <input asp-for="Subtitle_4_EN" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-12 mt-3">
                                        <h3 class="main-color">Success Message</h3>
                                    </div>
                                    <div class="col-12 col-md-6">
                                        <div class="form-group">
                                            <label asp-for="SuccessMessage_AZ" class="control-label"></label>
                                            <input asp-for="SuccessMessage_AZ" class="form-control" />
