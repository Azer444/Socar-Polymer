@model ContactFormComponent
<!-- Page Wrapper -->
<div id="wrapper">
    @await Html.PartialAsync("_SidebarPartial")
    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">
        <!-- Main Content -->
        <div id="content">
            @await Html.PartialAsync("_TopbarPartial")
            <!-- Begin Page Content -->
            <div class="container-fluid">
                <div class="card shadow mb-4">
                    <div class="card-header main-background py-3 d-flex align-items-center justify-content-between">
                        <h6 class="m-0 font-weight-bold main-color">Contact Form Component Details</h6>
                        <div>
                            <a asp-action="index" asp-controller="contact" class="btn btn-primary">Back to List</a>
                            <a asp-action="edit_form_component" asp-controller="contact" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-12 my-3">
                                <div class="row">
                                    <div class="col-6 font-weight-bold main-color">
                                        <h5>@Html.DisplayNameFor(model => model.Title_AZ)</h5>
                                    </div>
                                    <div class="col-6">
                                        @Html.Raw(Model.Title_AZ)
                                    </div>
                                    <div class="col-6 font-weight-bold main-color">
                                        <h5>@Html.DisplayNameFor(model => model.Title_RU)</h5>
                                    </div>
                                    <div class="col-6">
                                        @Html.Raw(Model.Title_RU)
                                    </div>
                                    <div class="col-6 font-weight-bold main-color">
                                        <h5>@Html.DisplayNameFor(model => model.Title_EN)</h5>
                                    </div>
                                    <div class="col-6">
                                        @Html.Raw(Model.Title_EN)
                                    </div>
                                    <div class="col-6 font-weight-bold main-color">
                                        <h5>@Html.DisplayNameFor(model => model.Subtitle_1_AZ)</h5>
                                    </div>
                                    <div class="col-6">
                                        @Html.Raw(Model.Subtitle_1_AZ)
                                    </div>
                                    <div class="col-6 font-weight-bold main-color">
                                        <h5>@Html.DisplayNameFor(model => model.Subtitle_1_RU)</h5>
                                    </div>
                                    <div class="col-6">
                                        @Html.Raw(Model.Subtitle_1_RU)
                                    </div>
                                    <div class="col-6 font-weight-bold main-color">
                                        <h5>@Html.DisplayNameFor(model => model.Subtitle_1_EN)</h5>
                                    </div>
                                    <div class="col-6">
                                        @Html.Raw(Model.Subtitle_1_EN)
                                    </div>
                                    <div class="col-6 font-weight-bold main-color">
                                        <h5>@Html.DisplayNameFor(model => model.Subtitle_2_AZ)</h5>
                                    </div>
                                    <div class="col-6">
                                        @Html.Raw(Model.Subtitle_2_AZ)
                                    </div>
                                    <div class="col-6 font-weight-bold main-color">
                                        <h5>@Html.DisplayNameFor(model => model.Subtitle_2_RU)</h5>
                                    </div>
                                    <div class="col-6">
                                        @Html.Raw(Model.Subtitle_2_RU)
                                    </div>
                                    <div class="col-6 font-weight-bold main-color">
                                        <h5>@Html.DisplayNameFor(model => model.Subtitle_2_EN)</h5>
                                    </div>
                                    <div class="col-6">
                                        @Html.Raw(Model.Subtitle_2_EN)
